{
  "name": "@filmcalendar/agents-uk",
  "description": "UK agents",
  "version": "0.0.0-development",
  "repository": "git@github.com:filmcalendar/agents-uk.git",
  "author": "Rui Costa",
  "license": "MIT",
  "keywords": [],
  "bin": {
    "fc-agent": ".bin/fc-agent"
  },
  "dependencies": {
    "@filmcalendar/agents-core": "1.1.0",
    "@filmcalendar/types": "1.0.0",
    "@sindresorhus/slugify": "1.1.0",
    "@tuplo/fletch": "1.3.0",
    "@tuplo/series-with": "1.0.0",
    "@tuplo/split-names-list": "1.3.0",
    "cheerio": "1.0.0-rc.3",
    "date-fns": "2.16.1",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@types/cheerio": "0.22.22",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.9",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "jest": "26.6.3",
    "nock": "13.0.5",
    "prettier": "2.2.0",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.33.3",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-typescript2": "0.29.0",
    "ts-jest": "26.4.4",
    "typescript": "4.1.2"
  },
  "scripts": {
    "build": "rimraf .bin; NODE_ENV=production rollup -c; chmod 755 .bin/fc-agent",
    "ci:test": "jest --ci",
    "ci:test:coverage": "jest --ci --coverage",
    "dev": "rollup -c -w",
    "lint": "eslint 'src/**'",
    "dev-run": "NODE_ENV=development fc-agent",
    "test": "jest --watch",
    "ts-check": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    }
  }
}
