{
  "name": "@filmcalendar/agents-uk",
  "description": "UK agents",
  "version": "0.0.0-development",
  "repository": "git@github.com:filmcalendar/agents-uk.git",
  "author": "Rui Costa",
  "license": "MIT",
  "keywords": [],
  "bin": {
    "fc-agent": ".bin/fc-agent"
  },
  "dependencies": {
    "@filmcalendar/agents-core": "1.8.0",
    "@filmcalendar/types": "1.2.0",
    "@sindresorhus/slugify": "1.1.0",
    "@tuplo/fletch": "1.4.0",
    "@tuplo/series-with": "1.0.0",
    "@tuplo/split-names-list": "1.4.0",
    "cheerio": "1.0.0-rc.3",
    "date-fns": "2.16.1",
    "lodash.escaperegexp": "4.1.2",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.0.0",
    "@types/cheerio": "0.22.23",
    "@types/jest": "26.0.18",
    "@types/lodash.escaperegexp": "4.1.6",
    "@types/node": "14.14.11",
    "@types/node-fetch": "2.5.7",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "env-cmd": "10.1.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "nock": "13.0.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.34.2",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-typescript2": "0.29.0",
    "ts-jest": "26.4.4",
    "typescript": "4.1.2"
  },
  "scripts": {
    "build": "rimraf .bin; NODE_ENV=production rollup -c; chmod 755 .bin/fc-agent",
    "ci:test": "jest --ci",
    "ci:test:coverage": "jest --ci --coverage",
    "dev": "rollup -c -w",
    "docker:rm-all": "docker rm $(docker ps -a -q)",
    "docker:rmi-all": "docker rmi -f $(docker images -q)",
    "docker:run": "env-cmd -f .env.secrets fc-agents-docker-run-local",
    "lint": "eslint src/**/*.ts",
    "dev-run": "NODE_ENV=development fc-agent",
    "test": "jest --watch",
    "ts-check": "tsc --noEmit",
    "validate": "yarn lint; yarn ts-check; yarn ci:test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb-typescript/base",
      "prettier",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/typescript",
      "plugin:jest/all"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "project": "./tsconfig.json"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "jest/no-hooks": "off",
      "@typescript-eslint/comma-dangle": "off",
      "no-underscore-dangle": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "no-undef": "off",
          "@typescript-eslint/naming-convention": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github"
    ]
  }
}
